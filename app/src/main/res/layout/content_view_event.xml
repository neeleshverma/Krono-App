<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.leo.krono.activities.ViewEvent"
    tools:showIn="@layout/activity_view_event">

    <TextView
        android:id="@+id/eview_desc"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:gravity="left|center_vertical"
        android:maxHeight="45dp"

        android:text="TextView"
        android:textColor="#000"
        android:textSize="17sp"
        app:layout_constraintBottom_toTopOf="@+id/eview_venue"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintLeft_toRightOf="@+id/imageView4"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/eview_notes"
        android:textSize="17sp"

        android:layout_width="0dp"
        android:textColor="#000"

        android:layout_height="0dp"
        android:layout_marginLeft="8dp"

        android:gravity="left|center_vertical"
        android:maxHeight="45dp"
        android:text="TextView"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/eview_cal"
        app:layout_constraintLeft_toRightOf="@+id/imageView8"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eview_end" />

    <TextView
        android:id="@+id/eview_start"
        android:layout_width="0dp"
        android:textSize="17sp"

        android:maxHeight="45dp"
        android:textColor="#000"

        android:layout_height="0dp"
        android:gravity="left|center_vertical"

        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/eview_end"
        app:layout_constraintTop_toBottomOf="@+id/eview_venue"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imageView2"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" />

    <TextView
        android:maxHeight="45dp"
        android:id="@+id/eview_end"
        android:layout_width="0dp"
        android:gravity="left|center_vertical"
        android:textSize="17sp"

        android:layout_height="0dp"
        android:textColor="#000"

        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/eview_notes"
        app:layout_constraintTop_toBottomOf="@+id/eview_start"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imageView7"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.498"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/eview_venue"
        android:layout_width="0dp"
        android:textColor="#000"

        android:layout_height="0dp"
        android:gravity="left|center_vertical"
        android:textSize="17sp"


        android:text="TextView"
        android:maxHeight="45dp"
        app:layout_constraintBottom_toTopOf="@+id/eview_start"
        app:layout_constraintTop_toBottomOf="@+id/eview_desc"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imageView5"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/eview_cal"
        android:textColor="#000"

        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="left|center_vertical"

        android:text="TextView"
        android:textSize="17sp"

        app:layout_constraintBottom_toTopOf="@+id/eview_min"
        app:layout_constraintTop_toBottomOf="@+id/eview_notes"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imageView9"
        android:maxHeight="45dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/eview_min"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="TextView"
        android:textSize="17sp"

        app:layout_constraintBottom_toTopOf="@+id/eview_repeat"
        android:textColor="#000"

        android:gravity="left|center_vertical"
        android:maxHeight="45dp"
        app:layout_constraintTop_toBottomOf="@+id/eview_cal"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imageView10"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.496"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/eview_repeat"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:maxHeight="45dp"
        android:gravity="left|center_vertical"
        android:textSize="17sp"

        android:textColor="#000"

        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eview_min"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imageView6"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:textSize="17sp"

        android:layout_marginLeft="8dp"

        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/eview_start"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/eview_start"
        app:srcCompat="@mipmap/ic_clock" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="7dp"
        android:layout_marginStart="7dp"
        app:layout_constraintBottom_toBottomOf="@+id/eview_venue"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/eview_venue"
        app:srcCompat="@mipmap/ic_venue"
        app:layout_constraintVertical_bias="0.434" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/eview_desc"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/eview_desc"
        app:srcCompat="@mipmap/ic_desc" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/eview_repeat"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/eview_repeat"
        app:srcCompat="@mipmap/ic_repeat" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/eview_min"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/eview_min"
        app:srcCompat="@mipmap/ic_alarm" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/eview_cal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/eview_cal"
        app:srcCompat="@android:drawable/ic_menu_my_calendar" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/eview_notes"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/eview_notes"
        app:layout_constraintTop_toTopOf="@+id/eview_notes"
        app:layout_constraintVertical_bias="0.357"
        app:srcCompat="@mipmap/ic_appico" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/eview_end"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/eview_end"
        app:srcCompat="@mipmap/ic_clock" />

</android.support.constraint.ConstraintLayout>
